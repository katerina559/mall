<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.s3.mapper.PropertyMapper">
    <select id="getPropertyByCid" resultType="com.s3.pojo.Property">
        select * from property where propertyCategoryId = #{cid}
    </select>
</mapper>