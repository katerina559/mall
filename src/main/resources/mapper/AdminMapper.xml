<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.s3.mapper.AdminMapper">
    <select id="login" resultType="com.s3.pojo.Admin">
        select * from admin where adminName = #{name} and adminPassword = #{pwd}
    </select>
</mapper>